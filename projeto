#include <stdio.h>
#include <locale.h>
int main()
{
    int velocidade;
    int distancia;
    
    printf("digite a velocidade: ")
    scanf("%d",&velocidade);
    
    printf("digite a distancia : ");
    scanf("%d.2f", &distancia);
    
    printf("a sua velocidade e: %d" , velocidade/distancia);
    
    return 0;
}

    
